{{#compare btnType "==" "button"}}
  <button class="btn {{className}}" {{#if disabled}}disabled="disabled"{{/if}}>{{btnText}}</button>
{{/compare}}

{{#compare btnType "==" "link"}}
  <a href="/" class="btn {{className}}">{{btnText}}</a>
{{/compare}}

{{#compare btnType "==" "btnLink"}}
  <button class="{{className}}">{{btnText}}</button>
{{/compare}}